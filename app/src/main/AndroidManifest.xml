        <activity android:name=".signup.signUpAct" android:label="Register" ><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name=".SplashAct" android:label="My notes"></activity>
    </application>

</manifest>